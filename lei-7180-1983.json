      },
      {
        "kind": "context",
        "num": "3",
        "token": "28",
        "text": "Brasília, em 20 de dezembro de 1983; 162o da Independência e 95o da República.",
        "id": "Lei-7180-ctx-3",
        "ref": "Lei-7180-1983/ctx.3",
