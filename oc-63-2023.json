      {
        "kind": "section",
        "num": "1",
        "token": "12",
        "text": "Assunto: Orientações sobre prioridade das pessoas idosas",
        "id": "Oficio-Circular-63-2023-DCJ-DMAP-assunto",
        "ref": "Ofício-Circular-63-2023/12",
        "hash": "494a4618ff42e6c1"
      },
      {
        "kind": "context",
        "num": "10",
        "token": "13",
        "text": "Excelentíssimos Senhores Juízes e Excelentíssimas Senhoras Juízas com competência criminal,",
        "id": "Oficio-Circular-63-2023-DCJ-DMAP-ctx-10",
        "ref": "Ofício-Circular-63-2023/ctx.10",
        "hash": "c88f2aec5379b23d",
