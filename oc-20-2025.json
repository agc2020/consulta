      {
        "kind": "context",
        "num": "4",
        "token": "8",
        "text": "Ofício-Circular n° 20/2025 - DCGJ-DSE Autos n° 0113290-77.2024.8.16.6000",
        "id": "Ofício-Circular-20-ctx-4",
