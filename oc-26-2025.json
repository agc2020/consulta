      {
        "kind": "context",
        "num": "5",
        "token": "11",
        "text": "Ofício-Circular n° 26/2025 - GC Autos n° 0176885-50.2024.8.16.6000",
        "id": "Oficio-Circular-26-2025-GC-ctx-5",
