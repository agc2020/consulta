      {
        "kind": "context",
        "num": "5",
        "token": "8",
        "text": "Ofício-Circular no 94/2024 - DCJ-DSE Autos no 0043472-38.2024.8.16.6000",
        "id": "Oficio-Circular-94-2024-DCJ-DSE-ctx-5",
