      {
        "kind": "context",
        "num": "5",
        "token": "8",
        "text": "Ofício-Circular n° 112/2025 - CJ Autos n° 0033636-07.2025.8.16.6000",
        "id": "Ofício-Circular-112-ctx-5",
        "ref": "Ofício-Circular-112-2025/ctx.5",
