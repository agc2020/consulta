      {
        "kind": "context",
        "num": "5",
        "token": "9",
        "text": "Ofício-Circular n° 102/2025 - GCJ Autos n° 0036020-40.2025.8.16.6000",
        "id": "Ofício-Circular-102-ctx-5",
