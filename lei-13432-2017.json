        }
      },
      {
        "kind": "context",
        "num": "8",
        "token": "56",
        "text": "Dispõe sobre o exercício da profissão de detetive particular.",
        "id": "Lei-13432-ctx-8",
        "ref": "Lei-13432-2017/ctx.8",
        "hash": "93585ca2630a6f5b",
        "metadata": {
