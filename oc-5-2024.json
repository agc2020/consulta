      },
      {
        "kind": "section",
        "num": "1",
        "token": "6",
        "text": "Assunto: 1.Alteração 2.Corregedoria da Justiça 3.Foro Extrajudicial 4.Registro de Imóveis 5.Ofício Circular no 140/2013 6.Desconto 7.Emolumentos Devidos 8.Aquisição do Primeiro Imóvel 9.Sistema Financeiro de Habitação (SFH) 10.Sistema de Financiamento Imobiliário (SFI) 11.Lei no 6.015/1973",
        "id": "Oficio-Circular-140-2013-Desconto-assunto",
        "ref": "Ofício-Circular-5-2024/6",
        "hash": "6a1c08f0cd3dc78a"
      },
      {
        "kind": "context",
        "num": "5",
        "token": "7",
        "text": "Ementa: Assunto: Alteração do Ofício-Circular no 5/2024 - DCJ-DMAP",
        "id": "Oficio-Circular-140-2013-Desconto-ctx-5",
        "ref": "Ofício-Circular-5-2024/ctx.5",
        "hash": "8db2cb58580d0c33",
        "metadata": {
          "doc_id": "Oficio-Circular-140-2013-Desconto"
        }
      },
      {
        "kind": "context",
        "num": "6",
        "token": "8",
        "text": "Autos no 0077593-29.2023.8.16.6000",
        "id": "Oficio-Circular-140-2013-Desconto-ctx-6",
        "ref": "Ofício-Circular-5-2024/ctx.6",
        "hash": "1b9b18a889455618",
