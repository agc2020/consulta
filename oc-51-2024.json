      {
        "kind": "context",
        "num": "4",
        "token": "8",
        "text": "Ofício-Circular n° 51/2024 - DSE Autos n° 0064429-60.2024.8.16.6000",
        "id": "Oficio-Circular-51-2024-DSE-ctx-4",
        "ref": "Ofício-Circular-51-2024/ctx.4",
        "hash": "73190d67a0e5165a",
