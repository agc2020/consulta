      },
      {
        "kind": "context",
        "num": "10",
        "token": "49",
        "text": "--- Tabela 2 ---",
        "id": "Lei-7444-ctx-10",
        "ref": "Lei-7444-1985/ctx.10",
        "hash": "fb37e7bae5c73bdd",
        "metadata": {
